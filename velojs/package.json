{
    "name": "velojs",
    "version": "0.1.0",
    "type": "module",
    "bin": {
        "velojs": "./bin/velojs.js"
    },
    "scripts": {
        "typecheck": "tsc --noEmit"
    },
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./server": {
            "types": "./src/server.tsx",
            "import": "./src/server.tsx"
        },
        "./client": {
            "types": "./src/client.tsx",
            "import": "./src/client.tsx"
        },
        "./hooks": {
            "types": "./src/hooks.tsx",
            "import": "./src/hooks.tsx"
        },
        "./cookie": {
            "types": "./src/cookie.ts",
            "import": "./src/cookie.ts"
        },
        "./factory": {
            "types": "./src/factory.ts",
            "import": "./src/factory.ts"
        },
        "./vite": {
            "types": "./src/vite.ts",
            "import": "./src/vite.ts"
        },
        "./config": {
            "types": "./src/config.ts",
            "import": "./src/config.ts"
        }
    },
    "typesVersions": {
        "*": {
            "server": [
                "./src/server.tsx"
            ],
            "client": [
                "./src/client.tsx"
            ],
            "hooks": [
                "./src/hooks.tsx"
            ],
            "cookie": [
                "./src/cookie.ts"
            ],
            "factory": [
                "./src/factory.ts"
            ],
            "vite": [
                "./src/vite.ts"
            ],
            "config": [
                "./src/config.ts"
            ]
        }
    },
    "peerDependencies": {
        "@hono/vite-dev-server": "^0.24.0",
        "@preact/preset-vite": "^2.0.0",
        "@preact/signals": "^2.0.0",
        "hono": "^4.0.0",
        "preact": "^10.0.0",
        "vite": "^7.0.0",
        "wouter-preact": "^3.0.0"
    },
    "devDependencies": {
        "@babel/generator": "^7.28.5",
        "@babel/parser": "^7.28.5",
        "@babel/traverse": "^7.28.5",
        "@babel/types": "^7.28.5",
        "@types/babel__generator": "^7.27.0",
        "@types/babel__traverse": "^7.28.0"
    }
}
